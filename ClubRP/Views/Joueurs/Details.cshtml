@model ClubRP.Models.Joueur

@{
    ViewBag.Title = "Details";
}

<h2>@Html.DisplayName(ClubRP.Views.Shared.SharedRessources.Detail)</h2>

<div>
    <h4>Joueur</h4>
    <hr />
    <dl class="dl-horizontal">
        

        <dt>
            @Html.DisplayName(ClubRP.Views.Groupes.GroupeRessources.Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nom)
        </dd>

        <dt>
            @Html.DisplayName(ClubRP.Views.Groupes.GroupeRessources.GameMaster)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Maitre)
        </dd>

        <dt>
            @Html.DisplayName(ClubRP.Views.Groupes.GroupeRessources.Specialisation)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Specialisation)
        </dd>

        <dt>
            @Html.DisplayName(ClubRP.Views.Groupes.GroupeRessources.GroupID)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GroupeID)
        </dd>

        <dt>
            @Html.DisplayName(ClubRP.Views.Groupes.GroupeRessources.CharacterID)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PersonnageID)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink(ClubRP.Views.Shared.SharedRessources.Modify, "Edit", new { id = Model.JoueurID }) |
    @Html.ActionLink(ClubRP.Views.Shared.SharedRessources.BtnBack, "Index")
</p>
